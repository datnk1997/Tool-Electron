{
  "name": "backuptool",
  "version": "0.1.0",
  "private": true,
  "main": "electron.js",
  "dependencies": {
    "antd": "^3.26.7",
    "concurrently": "^5.0.2",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "electron": "^7.1.9",
    "electron-is-dev": "^1.1.0",
    "express": "^4.16.4",
    "node-sass": "^7.0.0",
    "pg": "^7.7.1",
    "react": "^16.6.3",
    "react-app-rewired": "^2.1.5",
    "react-dialog": "^1.0.2",
    "react-dom": "^16.6.3",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "react-scripts": "2.1.1",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "start-server": "node ./src/server.js",
    "electron": "electron .",
    "react": "react-scripts start",
    "start": "concurrently --kill-others \"cross-env BROWSER=none react-scripts start\" \"npm run electron\" \"npm run start-server\" ",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Shopping List\""
  },
  "eslintConfig": {
    "extends": [
      "plugin:prettier/recommended"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
